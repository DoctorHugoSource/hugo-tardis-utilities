"VertexLitGeneric"
{
	$basetexture "models/hugoextension/windows/brundoob/windows_white_dark.vtf"
	$surfaceprop "Glass"
	$selfillummask "brun/ext/windows_white"
	$selfillum 1
	$envmap "env_cubemap"
	$envmaptint "[0.5 0.5 0.5]"



	$EmissiveBlendEnabled         1
    $EmissiveBlendStrength         1
    $EmissiveBlendTexture         "vgui/white"
    $EmissiveBlendBaseTexture         "brun/ext/windows_white"
    $EmissiveBlendFlowTexture         "vgui/white"
    $EmissiveBlendTint                 " {0 0 0} "
